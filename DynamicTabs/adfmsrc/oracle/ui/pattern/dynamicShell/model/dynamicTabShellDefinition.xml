                   xmlns="http://xmlns.oracle.com/adfm/uimodel"/>
      </parameters>
    </taskFlow>
  </executables>
  <bindings>
    <methodAction id="handleMessage"
                  InstanceName="DynamicShellHelper.dataProvider"
                  DataControl="DynamicShellHelper" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="handleMessage"
                  IsViewObjectMethod="false">
      <NamedData NDName="tabContext"
                 NDType="oracle.ui.pattern.dynamicShell.TabContext"/>
      <NamedData NDName="messenger"
                 NDType="soadev.ext.adf.taskflows.helper.Messenger"/>
    </methodAction>
    <methodAction id="launchActivity"
                  InstanceName="DynamicShellHelper.dataProvider"
                  DataControl="DynamicShellHelper" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="launchActivity"
                  IsViewObjectMethod="false">
      <NamedData NDName="tabContext"
                 NDType="oracle.ui.pattern.dynamicShell.TabContext"/>
      <NamedData NDName="title" NDType="java.lang.String"/>
      <NamedData NDName="taskFlowId" NDType="java.lang.String"/>
      <NamedData NDName="parameterMap" NDType="java.util.Map"/>
      <NamedData NDName="newTab" NDType="boolean"/>
    </methodAction>
    <methodAction id="markCurrentTabDirty"
                  InstanceName="DynamicShellHelper.dataProvider"
                  DataControl="DynamicShellHelper" RequiresUpdateModel="true"
                  Action="invokeMethod" MethodName="markCurrentTabDirty"
                  IsViewObjectMethod="false">
      <NamedData NDName="tabContext" NDValue=""
                 NDType="oracle.ui.pattern.dynamicShell.TabContext"/>
      <NamedData NDName="isDirty" NDValue="" NDType="java.lang.Boolean"/>
    </methodAction>
  </bindings>
  <eventMap xmlns="http://xmlns.oracle.com/adfm/contextualEvent">
    <event name="transDirtyEvent">
      <producer region="*">
        <consumer handler="markCurrentTabDirty">
          <parameters>
            <parameter name="tabContext" value="#{viewScope.tabContext}"/>
            <parameter name="isDirty" value="#{payLoad}"/>
          </parameters>
        </consumer>
      </producer>
    </event>
    <event name="messageEvent">
      <producer region="*">
        <consumer handler="handleMessage">
          <parameters>
            <parameter name="tabContext" value="#{viewScope.tabContext}"/>
            <parameter name="messenger" value="#{payLoad}"/>
          </parameters>
        </consumer>
      </producer>
    </event>
    <event name="launchActivityEvent">
      <producer region="*">
        <consumer handler="launchActivity">
          <parameters>
            <parameter name="tabContext" value="#{viewScope.tabContext}"/>
            <parameter name="title" value="#{payLoad.title}"/>
            <parameter name="taskFlowId" value="#{payLoad.taskFlowId}"/>
            <parameter name="parameterMap" value="#{payLoad.parameterMap}"/>
            <parameter name="newTab" value="true"/>
          </parameters>
        </consumer>
      </producer>
    </event>
  </eventMap>
</pageDefinition>
